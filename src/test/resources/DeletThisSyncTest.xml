<?xml version="1.0" encoding="UTF-8"?> 
<suite name="MyThrive" parallel="none" verbose="1">
	<listeners>
		<listener class-name="com.thrive.ui.core.AnnotationTransformer" />
	</listeners>
 
  <test name="User Authentication" parallel="methods" thread-count="1"> 
  	<parameter name="browser"  value="chrome"/>
    
	<classes>
	 	<class name="com.thrive.ui.test.categories.DeleteThisSynchTest"/>
	</classes>        
  </test> 
  
  <test name="User Authentication" parallel="methods" thread-count="1"> 
  	<parameter name="browser"  value="firefox"/>
    
	<classes>
	 	<class name="com.thrive.ui.test.categories.DeleteThisSynchTest"/>
	</classes>        
  </test> 
 
</suite> 
